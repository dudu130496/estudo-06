n = int(input('digite um numero inteiro:'))
m1 = n*1
m2 = n*2
m3 = n*3
m4 = n*4
m5 = n*5
m6 = n*6
m7 = n*7
m8 = n*8
m9 = n*9
m10 = n*10
print('A tabuada de {} e: \n1x = {} \n2x = {} \n3x = {} \n4x = {} \n5x = {} \n6x = {} \n7x = {} \n8x = {} \n9x = {} \n10x = {}'.format(n,m1,m2,m3,m4,m5,m6,m7,m8,m9,m10))